		<script>
		$(function(){
			
			// THIS SCRIPT IS FOR FRONTEND ONLY
			// {
			
			function callHeroBanner() {
				if($('.cycle-slideshow').length>0) {
					$( '.cycle-slideshow' ).cycle();
				}
			}
			
			var currentDate = new Date();
			var host = window.location.protocol + "://" + window.location.host + "/" + window.location.pathname;
			var pathArray = window.location.pathname.split( '/' );
			var page = "pages/"+ pathArray[2] +".html";
			
			if(pathArray[2]!==""){
				$("#site-container").load(page, function(response, status, xhr){
					if ( status == "error" ) {
						$("#site-container").load("pages/home.html?uid=" + currentDate.valueOf(), callHeroBanner);
					} else {
						callHeroBanner();
					}
					
				});
			} else {
				$("#site-container").load("pages/home.html", function(response, status, xhr){					
					callHeroBanner();					
				});
			}
			
			// }
			// FRONTEND SCRIPT END
			
		});
